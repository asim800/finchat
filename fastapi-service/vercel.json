{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "DATABASE_URL": "@database-url",
    "PRIMARY_ANALYSIS_BACKEND": "fastapi",
    "ENABLE_BACKEND_FALLBACK": "false"
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 30
    }
  }
}